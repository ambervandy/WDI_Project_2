<html>
<head>
	<title>ALL TRIPS</title>
	<link rel="stylesheet" type="text/css" href="/css/trips/tripIndex.css">
	<link href='https://fonts.googleapis.com/css?family=Lato' rel='stylesheet' type='text/css'>
	<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAF13D52tQ7DIimjcN7gRz1gkJTZElqPgw"></script>
	<script type="text/javascript" src="https://code.jquery.com/jquery-2.2.0.min.js"></script>
	<script type="text/javascript" src="/js/tripIndex.js"></script>
</head>
<body>

	<!-- nav bar -->
	<ul id="nav">
		<li><a href="/" class="navLi"><img id="logoImg" src="/images/logoBlack.png"></a></li>
		<li><a href="/users" class="navLi">TRAVELERS</a></li>
		<li><a href="/trips" class="navLi">TRIPS</a></li>
		<li>PROFILE</li>
		<li><a href="/users/logout" class="navLi">LOGOUT</a></li>
	</ul>

	<h1>ALL TRIPS</h1>

	<!-- trip index map -->
	<div id="mapContainer">
		<div id="mapImg"></div>
	</div>


	<!-- show list of all trips -->
		<ul id="tripList">
			<% for(var i = 0; i < trips.length; i++) { %>
				<li><a href="/trips/<%= trips[i].id %>"><%= trips[i].destination %></a></li>
			<% } %>
		</ul>

	<!-- hidden input for user id -->
	<input id="hidden" type="hidden" value="<%= trips.id %>">


<% include ../partials/footer %>

















